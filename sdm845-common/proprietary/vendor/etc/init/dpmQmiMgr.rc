#start dpmQmiMgr service
service dpmQmiMgr /system/vendor/bin/dpmQmiMgr
    class main
    user system
    group system
    disabled

on property:persist.vendor.dpmhalservice.enable=*
   start dpmQmiMgr

on property:persist.vendor.dpmhalservice.enable=0
   stop dpmQmiMgr
